  "description": "API automÃ¡tica para contenido semanal de reuniones JW.org mediante scraping",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "scrape": "node src/scraper.js",
    "validate": "node src/validator.js",
    "validate-openapi": "node src/openapi-validator.js",
    "generate-api": "node src/api-generator.js",
    "generate-languages": "node src/generate-languages-api.js",
    "generate-clients": "node src/client-generator.js",
    "summary": "node src/summary.js",
    "test": "node src/test.js",
    "dev": "node --watch src/scraper.js",
    "setup": "npm install && node src/setup.js",
    "preview": "node src/preview-server.js",
    "build": "npm run validate && npm run validate-openapi && npm run generate-languages && npm run generate-api && npm run generate-clients",
    "deploy-prep": "npm run build && cp openapi.yaml api/openapi.yaml"
  },
  "dependencies": {
